      <node id="313" label="Diamond detectors for high energy physics experiments">
        <attvalues>
          <attvalue for="0" value="10.1088/1748-0221/13/01/c01029"/>
          <attvalue for="1" value="313"/>
          <attvalue for="2" value="0"/>
