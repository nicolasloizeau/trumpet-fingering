        </attvalues>
        <viz:size value="1.0"/>
        <viz:position x="-155.08124" y="1728.1858"/>
        <viz:color r="192" g="192" b="192"/>
      </node>
      <node id="18" label="Fiber Bragg grating extensometers for reliable health monitoring applications in civil engineering">
        <attvalues>
