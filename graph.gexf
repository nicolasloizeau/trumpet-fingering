      <node id="60" label="Flexible Tactile Sensors for Multidigital Dexterous In‐Hand Manipulation">
        <attvalues>
          <attvalue for="0" value="10.1002/9781118572016.ch6"/>
          <attvalue for="1" value="60"/>
          <attvalue for="2" value="0"/>
