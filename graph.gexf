      <node id="285" label="BOOSTER: Development of a toolbox for triage of large group of individuals exposed to radioactive material">
        <attvalues>
          <attvalue for="0" value="10.1109/animma.2013.6727953"/>
          <attvalue for="1" value="285"/>
          <attvalue for="2" value="0"/>
